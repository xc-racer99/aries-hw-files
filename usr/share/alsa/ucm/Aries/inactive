# Use case Configuration for Mobile device
# By Inha Song <ideal.song@samsung.com>

SectionVerb {
  EnableSequence [
  ]
  DisableSequence [
  ]

  Value {
    TQ "Music"
    PlaybackCTL "hw:0"
    CaptureCTL "hw:0"
  }
}

SectionDevice."All" {
  Comment "reset all"

  EnableSequence [
    cset "name='Earpiece Switch' off"
    cset "name='RCV Switch' off"
    cset "name='SPK Switch' off"
    cset "name='HP Switch' off"
    cset "name='FM In Switch' off"
    cset "name='LINE Switch' off"
    cset "name='DAC1 Switch' off"
    cset "name='DAC2 Switch' off"
    cset "name='Speaker Switch' off"
    cset "name='Earpiece Switch' off"
    cset "name='Headphone Switch' off"
    cset "name='DAC1L Mixer AIF2 Switch' off"
    cset "name='DAC1R Mixer AIF2 Switch' off"
    cset "name='SPKL DAC1 Switch' off"
    cset "name='SPKR DAC1 Switch' off"
    cset "name='SPKL Boost SPKL Switch' off"
    cset "name='SPKL Boost SPKR Switch' off"
    cset "name='DAC1L Mixer AIF1.1 Switch' off"
    cset "name='DAC1R Mixer AIF1.1 Switch' off"
    cset "name='SPKR Boost SPKL Switch' off"
    cset "name='SPKR Boost SPKR Switch' off"
    cset "name='SPKR Input Switch' off"
    cset "name='SPKL Input Switch' off"
    cset "name='Earpiece Mixer Left Output Switch' off"
    cset "name='Earpiece Mixer Right Output Switch' off"
    cset "name='Right Output Mixer IN2RN Switch' off"
    cset "name='Right Output Mixer DAC Switch' off"
    cset "name='Left Output Mixer IN2LN Switch' off"
    cset "name='Left Output Mixer DAC Switch' off"
    cset "name='AIF2DAC2L Mixer AIF1.1 Switch' off"
    cset "name='AIF2DAC2R Mixer AIF1.1 Switch' off"
    cset "name='AIF2DAC2L Mixer AIF2 Switch' off"
    cset "name='AIF2DAC2R Mixer AIF2 Switch' off"
    cset "name='Speaker Mode' 0"
    cset "name='AIF2DAC2L Mixer AIF2 Switch' off"
    cset "name='AIF2DAC2R Mixer AIF2 Switch' off"
    cset "name='DAC1L Mixer Right Sidetone Switch' off"
    cset "name='DAC1R Mixer Right Sidetone Switch' off"
    cset "name='Main Mic Switch' off"
    cset "name='Headset Mic Switch' off"
    cset "name='MIXINR IN2R Switch' off"
    cset "name='MIXINL IN2L Switch' off"
    cset "name='IN2R PGA IN2RN Switch' off"
    cset "name='IN2L PGA IN2LN Switch' off"
    cset "name='IN2L Switch' off"
    cset "name='IN2R Switch' off"
    cset "name='IN1L Switch' off"
    cset "name='IN1R Switch' off"
    cset "name='MIXINR IN1R Switch' off"
    cset "name='MIXINL IN1L Switch' off"
    cset "name='IN1R PGA IN1RP Switch' off"
    cset "name='IN1R PGA IN1RN Switch' off"
    cset "name='IN1L PGA IN1LP Switch' off"
    cset "name='IN1L PGA IN1LN Switch' off"
    cset "name='AIF1ADC1L Mixer ADC/DMIC Switch' off"
    cset "name='AIF1ADC1R Mixer ADC/DMIC Switch' off"
  ]

  DisableSequence [
  ]
}
SectionDevice."Playback" {
  Comment "reset playback"

  EnableSequence [
    cset "name='Earpiece Switch' off"
    cset "name='RCV Switch' off"
    cset "name='SPK Switch' off"
    cset "name='HP Switch' off"
    cset "name='FM In Switch' off"
    cset "name='LINE Switch' off"
    cset "name='DAC1 Switch' off"
    cset "name='DAC2 Switch' off"
    cset "name='Speaker Switch' off"
    cset "name='Earpiece Switch' off"
    cset "name='Headphone Switch' off"
    cset "name='DAC1L Mixer AIF2 Switch' off"
    cset "name='DAC1R Mixer AIF2 Switch' off"
    cset "name='SPKL DAC1 Switch' off"
    cset "name='SPKR DAC1 Switch' off"
    cset "name='SPKL Boost SPKL Switch' off"
    cset "name='SPKL Boost SPKR Switch' off"
    cset "name='DAC1L Mixer AIF1.1 Switch' off"
    cset "name='DAC1R Mixer AIF1.1 Switch' off"
    cset "name='SPKR Boost SPKL Switch' off"
    cset "name='SPKR Boost SPKR Switch' off"
    cset "name='SPKR Input Switch' off"
    cset "name='SPKL Input Switch' off"
    cset "name='Earpiece Mixer Left Output Switch' off"
    cset "name='Earpiece Mixer Right Output Switch' off"
    cset "name='Right Output Mixer IN2RN Switch' off"
    cset "name='Right Output Mixer DAC Switch' off"
    cset "name='Left Output Mixer IN2LN Switch' off"
    cset "name='Left Output Mixer DAC Switch' off"
    cset "name='AIF2DAC2L Mixer AIF1.1 Switch' off"
    cset "name='AIF2DAC2R Mixer AIF1.1 Switch' off"
    cset "name='AIF2DAC2L Mixer AIF2 Switch' off"
    cset "name='AIF2DAC2R Mixer AIF2 Switch' off"
    cset "name='Speaker Mode' 0"
    cset "name='AIF2DAC2L Mixer AIF2 Switch' off"
    cset "name='AIF2DAC2R Mixer AIF2 Switch' off"
    cset "name='DAC1L Mixer Right Sidetone Switch' off"
    cset "name='DAC1R Mixer Right Sidetone Switch' off"
  ]

  DisableSequence [
  ]
}

SectionDevice."Capture" {
  Comment "reset capture"

  EnableSequence [
    cset "name='Right Output Mixer IN2RN Switch' off"
    cset "name='Left Output Mixer IN2LN Switch' off"
    cset "name='Main Mic Switch' off"
    cset "name='Headset Mic Switch' off"
    cset "name='MIXINR IN2R Switch' off"
    cset "name='MIXINL IN2L Switch' off"
    cset "name='IN2R PGA IN2RN Switch' off"
    cset "name='IN2L PGA IN2LN Switch' off"
    cset "name='IN2L Switch' off"
    cset "name='IN2R Switch' off"
    cset "name='IN1L Switch' off"
    cset "name='IN1R Switch' off"
    cset "name='MIXINR IN1R Switch' off"
    cset "name='MIXINL IN1L Switch' off"
    cset "name='IN1R PGA IN1RP Switch' off"
    cset "name='IN1R PGA IN1RN Switch' off"
    cset "name='IN1L PGA IN1LP Switch' off"
    cset "name='IN1L PGA IN1LN Switch' off"
    cset "name='AIF1ADC1L Mixer ADC/DMIC Switch' off"
    cset "name='AIF1ADC1R Mixer ADC/DMIC Switch' off"
  ]

  DisableSequence [
  ]
}

